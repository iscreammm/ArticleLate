{
  "name": "client",
  "version": "0.1.0",
  "type": "module",
  "module": "commonjs",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.1.3",
    "cypress-file-upload": "^5.0.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.3",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "npx cypress run && react-scripts test && CI=false react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --env=jsdom --watchAll=false --coverage",
    "test:start": "react-scripts test --env=jsdom --watchAll=false --coverage && react-scripts start",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.jsx",
      "!src/components/App.jsx",
      "!src/components/utilities/RequireAuth.jsx",
      "!src/components/utilities/userContext.jsx",
      "!src/pages/LoginPage.jsx",
      "!src/pages/NotFound.jsx"
    ]
  },
  "devDependencies": {
    "@testing-library/cypress": "^9.0.0",
    "canvas": "^2.11.0",
    "cypress": "^12.9.0"
  }
}
